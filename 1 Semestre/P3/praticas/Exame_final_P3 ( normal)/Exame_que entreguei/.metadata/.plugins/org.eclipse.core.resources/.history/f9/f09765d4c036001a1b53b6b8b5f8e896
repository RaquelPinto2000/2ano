import java.util.*;
public class Banco extends Cliente{
	private String nome ;
	private List<String>  s = new ArrayList<String>();
	public Banco(String nome) {
		super(nome);
		this.nome=nome;
	}
	
	public boolean add(String s) {
		boolean b=false;
		for(int i=0 ;i<s.size();i++) {
			for(int j = i ;j<s.size();j++) {
				if(s.get(i)==s.get(j)) {
					b= false;
					break;
				}else {
					b=true;
					s.add(nome);
				}
			}
		}
		return b;
	
	}
	
}
